{
  "name": "konstantinkuklin/doctrine-compressed-fields-bundle",
  "description": "Symfony DoctrineCompressedFieldsBundle",
  "keywords": ["database", "doctrine", "symfony"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Kuklin",
      "email": "konstantin.kuklin@gmail.com"
    }
